{
  "name": "mycoverage",
  "version": "1.30.3",
  "scripts": {
    "dev": "blitz dev",
    "dev:worker": "nodemon --exec 'npm run build:worker && npm run start:worker' src/worker.ts",
    "build:worker": "webpack --config ./worker.webpack.config.js",
    "start:worker": "DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config ./dist/worker.js",
    "build": "blitz build",
    "build:protobuf": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./ src/library/proto_coverage.proto",
    "start:frontend": "blitz start",
    "output_version": "awk '/version/{gsub(/(\"|\",)/,\"\",$2);print $2}' package.json",
    "release:publish": "npm run release && docker build -t aeolun/mycoverage:latest -t aeolun/mycoverage:$(npm run output_version) .",
    "start": "node concurrently.js",
    "studio": "blitz prisma studio",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "test:tsc": "tsc --noEmit",
    "lint": "biome check src && rome format src",
    "lint:fix": "biome check --apply-unsafe src && biome format --apply-unsafe src",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "housekeeping": "node housekeeping.mjs",
    "healthcheck": "node healthchecks.mjs",
    "prepare": "husky install && sed -i '34 i \"./core/*\": \"./core/*.js\",' node_modules/next-auth/package.json"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "rome check",
      "rome format"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
    "@blitzjs/auth": "2.0.0-beta.33",
    "@blitzjs/next": "2.0.0-beta.33",
    "@blitzjs/rpc": "2.0.0-beta.33",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/system": "^2.6.1",
    "@chakra-ui/theme": "^3.3.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@nivo/axes": "0.74.1",
    "@nivo/core": "^0.74.1",
    "@nivo/line": "0.74.1",
    "@nivo/tooltip": "^0.74.1",
    "@nivo/treemap": "^0.74.1",
    "@octokit/auth-app": "^4.0.13",
    "@octokit/rest": "^19.0.13",
    "@prisma/client": "^5.4.2",
    "@react-spring/web": "9.7.2",
    "ajv": "8.12.0",
    "axios": "^0.21.4",
    "blitz": "2.0.0-beta.33",
    "bullmq": "^4.12.3",
    "classnames": "2.3.2",
    "concurrently": "^7.6.0",
    "d3-scale": "4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "dotenv": "^10.0.0",
    "final-form": "^4.20.10",
    "framer-motion": "^5.6.0",
    "jsonwebtoken": "^8.5.1",
    "long": "5.2.3",
    "memoize-one": "6.0.0",
    "minimist": "^1.2.8",
    "next": "13.4.19",
    "next-auth": "^4.23.2",
    "prismjs": "1.29.0",
    "prop-types": "^15.8.1",
    "protobufjs": "^6.11.4",
    "queue": "^6.0.2",
    "react": "^18.2.0",
    "react-datetime": "^3.2.0",
    "react-diff-viewer-continued": "^4.0.1",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-icons": "^4.11.0",
    "react-timeago": "7.1.0",
    "uuidv7": "0.6.3",
    "xml2js": "^0.4.23",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.1",
    "@codedependant/semantic-release-docker": "^4.4.0",
    "@next/env": "^13.5.4",
    "@octokit/webhooks-types": "^7.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.8.4",
    "@types/preview-email": "^2.0.1",
    "@types/react": "^18.2.28",
    "@types/react-vis": "^1.11.12",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-istanbul": "0.31.0",
    "dotenv-cli": "7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "8.8.0",
    "husky": "^6.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "k6": "^0.0.0",
    "lint-staged": "^10.5.4",
    "mycoverage-cli": "^0.0.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^0.12.1",
    "pretty-quick": "^3.1.3",
    "preview-email": "^3.0.19",
    "prisma": "^5.4.2",
    "sass": "^1.69.2",
    "semantic-release": "^19.0.5",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.161.1",
    "tsconfig-paths": "^3.14.2",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "private": true
}
