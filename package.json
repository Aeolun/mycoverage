{
  "name": "mycoverage",
  "version": "1.6.0",
  "scripts": {
    "dev": "blitz dev",
    "dev:worker": "nodemon --exec 'npm run build:worker && npm run start:worker' app/worker.ts",
    "build:worker": "webpack --config ./worker.webpack.config.js",
    "start:worker": "DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config ./dist/worker.js",
    "build": "blitz build",
    "build:protobuf": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./ ./app/library/proto_coverage.proto",
    "start:frontend": "blitz start",
    "output_version": "awk '/version/{gsub(/(\"|\",)/,\"\",$2);print $2}' package.json",
    "release:publish": "npm run release && docker build -t aeolun/mycoverage:latest -t aeolun/mycoverage:$(npm run output_version) .",
    "start": "node concurrently.js",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test:tsc": "tsc --noEmit",
    "pretty": "prettier .",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.15",
    "@chakra-ui/react": "1.1.2",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@nivo/core": "0.74.0",
    "@nivo/treemap": "0.74.0",
    "@prisma/client": "3.15.2",
    "aws-sdk": "2.1041.0",
    "axios": "0.21.1",
    "blitz": "0.45.4",
    "bullmq": "1.76.2",
    "concurrently": "7.2.2",
    "d3-scale-chromatic": "3.0.0",
    "dotenv": "^10.0.0",
    "final-form": "4.20.2",
    "framer-motion": "3.2.0",
    "joi": "17.4.0",
    "minimist": "1.2.6",
    "protobufjs": "6.11.2",
    "queue": "6.0.2",
    "react": "18.0.0-alpha-568dc3532",
    "react-d3-treemap": "1.0.29",
    "react-diff-viewer": "3.1.1",
    "react-dom": "18.0.0-alpha-568dc3532",
    "react-final-form": "6.5.3",
    "react-icons": "4.2.0",
    "xml2js": "^0.4.23",
    "zod": "3.5.1"
  },
  "devDependencies": {
    "@codedependant/semantic-release-docker": "4.1.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/bull": "3.15.8",
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.11",
    "@types/react-vis": "1.11.10",
    "@types/xml2js": "0.4.9",
    "dotenv": "10.0.0",
    "eslint": "7.28.0",
    "husky": "6.0.0",
    "jest": "28.1.2",
    "jest-environment-jsdom": "28.1.2",
    "k6": "0.0.0",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.15",
    "prettier": "2.3.1",
    "prettier-plugin-prisma": "0.12.1",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4",
    "prisma": "3.15.2",
    "sass": "1.43.4",
    "standard-version": "9.3.0",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "ts-proto": "1.97.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.3.5",
    "webpack": "5.51.1",
    "webpack-cli": "4.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "private": true
}
